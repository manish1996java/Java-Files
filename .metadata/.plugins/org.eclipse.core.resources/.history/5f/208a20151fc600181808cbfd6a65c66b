package com.bmpl.test;

import java.io.File;

public class FileFilter {
	public static void main(String args[])
	{
		File f = new File("D:\\");
		
		System.out.println(f.getAbsolutePath());
		
		String txt[] = f.list();
		
		for(int i=0;i<=txt.length;i++)
		{
			System.out.println(txt[i]);
		}
	}

}
